# Domain configuration
DOMAIN_NAME=osamusic.org
DOMAIN_ALIASES=www.osamusic.org

# OpenAI API configuration
OPENAI_API_KEY=your_openai_api_key_here
OPENAI_MODEL=gpt-4o-mini

# Authentication secrets
ADMIN_REGISTRATION_SECRET=admin123
USER_REGISTRATION_SECRET=user123

# Firebase configuration
FIREBASE_API_KEY=your_firebase_api_key_here
FIREBASE_SERVICE_ACCOUNT_JSON=your_firebase_service_account_json_here

# Google OAuth
VITE_GOOGLE_CLIENT_ID=your_google_client_id_here

# Cloud SQL Configuration
CLOUD_SQL_CONNECTION_NAME=your-project:region:instance-name
DB_USER=your_db_user
DB_PASSWORD=your_db_password
DB_NAME=your_database_name
# DATABASE_URL will be automatically constructed from the above variables

# Other settings
MAX_DOCUMENT_SIZE=4000